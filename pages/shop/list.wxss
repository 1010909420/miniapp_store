.good-list {
  float: left;
  padding-left: 20rpx;
  padding-bottom: 20rpx;
}

.goodlist {
  margin-top: 20rpx;
}

.good-list-photo {
  width: 340rpx;
  height: 340rpx;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

.good-list-info {
  width: 340rpx;
  height: 475rpx;
  border: 1px solid #d7d7d7;
  border-radius: 5px;
}

.good-list-desc {
  width: 300rpx;
  height: 60rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.good-list-money {
  padding-left: 20rpx
}

#good-name {
  font-size: 28rpx;
}

#good-money {
  font-size: 33rpx;
  color: #ff4400;
  font-weight: bold;
}

#good-quantity {
  margin-left: 8rpx;
  font-size: 20rpx;
  color: #888888;
}
